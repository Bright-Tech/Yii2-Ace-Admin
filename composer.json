{
    "name": "bright-tech/yii2-ace-admin-theme",
    "description": "为Yii2 开发的 Ace-Admin主题组件库",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","ace-admin"],
    "license": "GNU GENERAL PUBLIC LICENSE",
    "authors": [
        {
            "name": "BrightTech",
            "email": "xiao.yanqi@bright-tech.cn"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "*",
        "bright-tech/ace-admin": "*"
    },
    "autoload": {
        "psr-4": {
            "bright_tech\\yii2theme\\aceadmin\\": "src/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.oschina.net/brightech/AceAdmin"
        },
        {
            "type": "vcs",
            "url": "https://git.oschina.net/brightech/InspiniaAdmin"
        }
    ]
}
